[{"name":"keyField","type":"String","required":"true","default":null,"description":"选项唯一标识字段"},{"name":"childrenField","type":"String","required":false,"default":null,"description":"用于展示的子集字段名称","tip":"用于分组展示"},{"name":"store","type":"Array","required":"true","default":null,"description":"数据源"},{"name":"pageSize","type":"Number","required":false,"default":"20","description":"（默认）单页展示条数"},{"name":"customHeader","type":"Function","required":false,"default":null,"description":"自定义展示分组头部","tip":"childrenField被指定时启用，自定义头部可返回html片段，因此需调用时注意xss漏洞处理"},{"name":"remote","type":"Function","required":false,"default":null,"description":"远程搜索函数, 接收参数为匹配文本","tip":"外部使用时候，建议做数据查询缓存，以提高性能"},{"name":"theme","type":"String","required":false,"default":"'normal'","description":"同select"},{"name":"value","type":"[Array,Object]","required":false,"default":null,"description":"value","tip":"如果multiple为true，则value需要为对象数组，否则为Object，Object需是Store的子项，或者至少拥有有效的keyField字段"},{"name":"width","type":"Number","required":false,"default":"150","description":"下拉框宽度"},{"name":"labelField","type":"String","required":"true","default":null,"description":"用于展示的字段名称"},{"name":"placeholder","type":"String","required":false,"default":"'请选择'","description":"默认提示文本"},{"name":"noMatchText","type":"String","required":false,"default":"'无匹配数据'","description":"筛选无匹配数据的提示文本","tip":"filterOption开启后生效"},{"name":"noDataText","type":"String","required":false,"default":"'暂无数据'","description":"无数据的提示文本"},{"name":"clearAble","type":"Boolean","required":false,"default":"true","description":"可清空选中结果"},{"name":"filterOption","type":"Boolean","required":false,"default":"false","description":"开启选项筛选过滤"},{"name":"multiple","type":"Boolean","required":false,"default":"false","description":"多选模式"}]