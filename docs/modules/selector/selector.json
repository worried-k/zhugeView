[{"name":"keyField","type":"String","required":"true","default":null,"description":"选项唯一标识字段"},{"name":"childrenField","type":"String","required":false,"default":null,"description":"用于展示的子集字段名称","tip":"用于分组展示"},{"name":"store","type":"Array","required":"true","default":null,"description":"数据源"},{"name":"disableOptions","type":"Array","required":false,"default":"[]","description":"禁用的选项列表：key值列表"},{"name":"pageSize","type":"Number","required":false,"default":"20","description":"单页展示条数,单页展示条数将影响组件性能，建议不要太大值"},{"name":"theme","type":"String","required":false,"default":"'normal'","description":"主题，目前支持的主题有：normal、noborder"},{"name":"value","type":"[Array,Object]","required":false,"default":null,"description":"通过v-model的形式进行数据绑定，不建议直接传递value属性","tip":"如果multiple为true，则value需要为对象数组，否则为Object，Object需是Store的子项，或者至少拥有有效的keyField字段"},{"name":"width","type":"Number","required":false,"default":"150","description":"下拉框宽度。如果theme为noborder，则该值为最大宽度，组件宽度在范围内自适应"},{"name":"labelField","type":"String","required":"true","default":null,"description":"用于展示的字段名称"},{"name":"placeholder","type":"String","required":false,"default":"'请选择'","description":"默认提示文本"},{"name":"noMatchText","type":"String","required":false,"default":"'无匹配数据'","description":"筛选无匹配数据的提示文本","tip":"filterOption开启后生效"},{"name":"noDataText","type":"String","required":false,"default":"'暂无数据'","description":"无数据的提示文本"},{"name":"clearAble","type":"Boolean","required":false,"default":"true","description":"可清空选中结果","tip":"multiple模式下有效"},{"name":"filterOption","type":"Boolean","required":false,"default":"false","description":"开启选项筛选过滤"},{"name":"multiple","type":"Boolean","required":false,"default":"false","description":"多选模式"}]